{{> nav}}

<div id="signersCity-container">
    <h3>All signers from {{city}}</h3>

    <ul>
        {{#each signers}}
            <li>
                <a href="{{url}}">{{first}} {{last}}</a> {{#if age}} | {{age}} {{/if}} {{#if city}} | {{city}} {{/if}}
            </li>
        {{/each}}
    </ul>
</div>